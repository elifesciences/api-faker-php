{
    "name": "elife/api-faker",
    "description": "eLife Sciences API faker",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "eLife\\ApiFaker\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "test\\eLife\\ApiFaker\\": "test/"
        }
    },
    "require": {
        "php": "^7.0",
        "beberlei/assert": "^2.6.7",
        "fzaninotto/faker": "^1.3"
    },
    "require-dev": {
        "elife/api": "dev-master",
        "justinrainbow/json-schema": "^2.0",
        "phpunit/phpunit": "^5.2",
        "puli/composer-plugin": "^1.0-beta9",
        "puli/repository": "^1.0-beta9",
        "webmozart/json": "^1.3@dev"
    },
    "minimum-stability": "beta",
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    }
}
